#include<iostream>
#include<string>
#include<vector>
#include<cstdlib>
using namespace std;
void keep();
struct Name_pair
{
	int age;
	string name;
};
void add_pair(vector<Name_pair> &a)//pass by reference
{
	Name_pair temp;//to store the data of new element
	string key;
	cout << "enter the name or enter exit to quit: "<<endl;
	cin >> key;
	while (key != "exit")//to judge if the process should end
	{
		a.push_back(temp);//after creating the new element,there's still sth to do in the loop
		key.clear();// clear the string before you update the key
	}
}
